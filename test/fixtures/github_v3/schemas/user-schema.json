        },
        "avatar_url": {
            "type": "string"
        },
        "gravatar_id": {
            "type": "string"
        },
        "url": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "company": {
            "type": "string"
        },
        "blog": {
            "type": "string"
        },
        "location": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "hireable": {
